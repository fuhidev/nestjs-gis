    "shp2json": "^1.4.0",
    "terraformer-arcgis-parser": "^1.1.0",
    "terraformer-wkt-parser": "^1.2.1"
  },
  "devDependencies": {
    "@nestjs/common": "^7.6.0",
    "@nestjs/core": "^7.6.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.6.0",
    "@nestjs/testing": "6.1.1",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjsx/crud": "^4.6.0",
    "@nestjsx/crud-typeorm": "^4.6.0",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/multer": "^1.4.7",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "exceljs": "^4.3.0",
    "geojson": "^0.5.0",
    "jest": "24.7.1",
    "mssql": "^7.2.1",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prettier": "1.17.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
