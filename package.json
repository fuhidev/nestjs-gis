{
  "name": "@gis/nestjs",
  "version": "1.0.0",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "npx vite build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "main": "./dist/gisjs.js",
  "module": "./dist/gisjs.mjs",
  "typings": "./dist/gisjs.d.ts",
  "exports": {
    ".": {
      "import": "./dist/gisjs.mjs",
      "require": "./dist/gisjs.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": ">=6.0.0",
    "@nestjs/jwt": ">=6.0.0",
    "@nestjsx/crud": ">=4.6.0",
    "@nestjsx/crud-typeorm": ">=4.6.0",
    "exceljs": ">=4.3.0",
    "geojson": ">=0.5.0",
    "reflect-metadata": ">=0.1.13",
    "rxjs": ">=6.0.0",
    "typeorm": ">=0.2.0"
  },
  "dependencies": {
    "geojson": ">=0.5.0",
    "terraformer-arcgis-parser": ">=1.1.0",
    "terraformer-wkt-parser": ">=1.2.1"
  },
  "devDependencies": {
    "@nestjs/common": "^7.6.0",
    "@nestjs/core": "^7.6.0",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjsx/crud": "^4.6.0",
    "@nestjsx/crud-typeorm": "^4.6.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "prettier": "1.17.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "supertest": "4.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typeorm": "^0.3.1",
    "typescript": "^4.7.4",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
