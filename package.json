    "@nestjsx/crud-typeorm": "^4.6.0 || ^5.0.0",
    "bcrypt": "^4.0.0",
    "exceljs": "^4.3.0",
    "geojson": "^0.5.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0 || ^7.0.0",
    "terraformer-arcgis-parser": "^1.1.0",
    "terraformer-wkt-parser": "^1.2.1",
    "typeorm": "^0.2.0 || ^0.3.0"
